{
  "bracketSpacing": true,
  "semi": false,
  "singleQuote": false,
  "singleAttributePerLine": true,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",

  "importOrderMergeDuplicateImports": true,
  "importOrderCombineTypeAndValueImports": true,

  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}
